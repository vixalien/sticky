msgctxt "shortcut window"
msgid "Delete note"
msgstr "குறிப்பை நீக்கு"

#: data/ui/help-overlay.ui:45
msgctxt "shortcut window"
msgid "Navigation"
msgstr "வழிசெலுத்தல்"

#: data/ui/help-overlay.ui:48
msgctxt "shortcut window"
msgid "Show all notes"
msgstr "எல்லா குறிப்புகளையும் காட்டு"

#: data/ui/help-overlay.ui:54
msgctxt "shortcut window"
msgid "Cycle between notes"
msgstr "குறிப்புகள் இடையே சுழற்சி"

#: data/ui/help-overlay.ui:60
msgctxt "shortcut window"
msgid "Cycle between notes in reverse order"
msgstr "தலைகீழ் வரிசையில் குறிப்புகளுக்கு இடையில் சுழற்சி"

#: data/ui/help-overlay.ui:68
msgctxt "shortcut window"
msgid "Editing"
msgstr "திருத்துதல்"

#: data/ui/help-overlay.ui:71
msgctxt "shortcut window"
msgid "Bold"
msgstr "தடித்த"

#: data/ui/help-overlay.ui:77
msgctxt "shortcut window"
